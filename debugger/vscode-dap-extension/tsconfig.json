{
	"compilerOptions": {
		"target": "ES2022",
		"module": "NodeNext",
		"moduleResolution": "NodeNext",
		"outDir": "out",
		"lib": [
			"ES2022"
		],
		"sourceMap": true,
		"strict": true,
		"noImplicitReturns": true,
		"noFallthroughCasesInSwitch": true,
		"noImplicitAny": false,
		"removeComments": false,
		"noUnusedLocals": true,
		"noImplicitThis": true,
		"inlineSourceMap": false,
		"preserveConstEnums": true,
		"strictNullChecks": true,
		"noUnusedParameters": false
	},
	"exclude": [
		"node_modules"
	]
}
