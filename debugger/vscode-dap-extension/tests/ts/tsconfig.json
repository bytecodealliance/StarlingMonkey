{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "es6",
        "target": "es2020",
        "jsx": "react",
        "skipLibCheck": true,
        "lib": [
            "ES2020",
            "WebWorker"
        ],
        "allowJs": true,
        "strict": true,
        "noImplicitReturns": true,
        "moduleResolution": "node"
    },
    "include": [
        "src/**/*"
    ]
}
